{
    "name": "@gotok/pass-book",
    "version": "0.0.4",
    "description": "Pass Book JavaScript API.",
    "main": "dist/src/pass-book.js",
    "types": "dist/src/pass-book.d.ts",
    "scripts": {
        "build:docs": "typedoc",
        "build": "rimraf dist && tsc -p tsconfig.json",
        "pretest": "yarn build",
        "start": "tsc --watch",
        "clean": "rm -rf lib",
        "prepare": "run-s clean build",
        "test": "esr ./test/*.test.ts",
        "lint": "eslint \"{src,test}/**/*.ts\" --format stylish",
        "fix:lint": "npm run lint --fix",
        "prettier": "prettier \"{src,test}/**/*.ts\" --check",
        "fix:prettier": "prettier --write src/",
        "fix": "npm run fix:lint && npm run fix:prettier"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "nft",
        "pass",
        "solana",
        "blockchain"
    ],
    "homepage": "https://gotok.app",
    "repository": "https://github.com/GotokHQ/pass-book.git",
    "author": "PassBook Maintainers <contact@gotok.app>",
    "license": "MIT",
    "dependencies": {
        "@metaplex-foundation/mpl-core": "^0.6.1",
        "@metaplex-foundation/mpl-token-metadata": "^1.2.5",
        "@solana/spl-token": "^0.1.8",
        "@solana/web3.js": "^1.41.3",
        "@types/bn.js": "^5.1.0",
        "bn.js": "^5.2.0"
    },
    "devDependencies": {
        "@metaplex-foundation/amman": "^0.0.12",
        "@types/debug": "^4.1.7",
        "@types/tape": "^4.13.2",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.4.0",
        "debug": "^4.3.3",
        "esbuild": "^0.14.27",
        "esbuild-runner": "2.2.1",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.4.1",
        "rimraf": "^3.0.2",
        "spok": "^1.4.2",
        "supports-color": "^9.2.1",
        "tape": "^5.3.2",
        "typescript": "^4.5.2"
    }
}
